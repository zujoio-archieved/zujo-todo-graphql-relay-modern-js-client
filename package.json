    "dotenv": "^7.0.0",
    "farce": "^0.2.8",
    "found": "^0.3.21",
    "found-relay": "^0.4.0-alpha.8",
    "install": "^0.12.2",
    "keycode": "^2.2.0",
    "npm": "^6.9.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.5",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.0.2",
    "react-icons": "^3.5.0",
    "react-relay": "3.0.0",
    "react-responsive-modal": "^3.6.0",
