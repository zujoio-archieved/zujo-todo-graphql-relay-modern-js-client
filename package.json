{
  "name": "frontend_boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for frontend",
  "main": "index.js",
  "scripts": {
    "relay-compiler": "relay-compiler --src src --exclude 'data/**' --schema ./data/schema.graphql --watch",
    "update-schema": "graphql get-schema --output ./data/schema.graphql --watch",
    "webpack": "webpack --mode production",
    "watch": "npm run update-schema | npm run relay-compiler",
    "start": "webpack-dev-server --host localhost --port 8080 --mode development"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.coruscate.in/zujo-team/frontend_boilerplate.git"
  },
  "author": "arjun.kava",
